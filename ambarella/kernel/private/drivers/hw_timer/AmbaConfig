config BUILD_AMBARELLA_HW_TIMER
depends on BUILD_AMBARELLA_PRIVATE_MODULES
	bool "Build Ambarella private hw_timer module"
	default n
	depends on CONFIG_ARCH_S2L
	help
		Build Ambarella private hardware timer module for Linux.

