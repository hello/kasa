menu "Network/BlueTooth Packages"

menu "Wireless"
config CONFIG_AMBARELLA_MARVELL_WIRELESS_SUPPORT
  bool "Add Marvell wireless driver and FW into root_fs"
  default n
  select CONFIG_AMBARELLA_WIRELESS_TOOL_SUPPORT
