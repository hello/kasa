menu "Audio Packages"

menu "ALSA"
config CONFIG_AMBARELLA_ALSA_SUPPORT
  select CONFIG_AMBARELLA_NCURSES_SUPPORT
    bool "Add ALSA library into root_fs"
    default y
    help
      Add ALSA library support in file system.

config CONFIG_AMBARELLA_ALSA_UTILS_SUPPORT
  depends on CONFIG_AMBARELLA_ALSA_SUPPORT
  bool "Add ALSA utils into root_fs"
  default y
  help
    Add ALSA tools into file system

config CONFIG_AMBARELLA_ALSA_SPEEKER_TEST_SUPPORT
  depends on CONFIG_AMBARELLA_ALSA_UTILS_SUPPORT
  bool "Add ALSA speaker test program and sounds into root_fs"
  default n
  help
    Add ALSA speaker test program and sounds into root_fs

config CONFIG_AMBARELLA_ALSA_PLUGINS_SUPPORT
  depends on CONFIG_AMBARELLA_ALSA_SUPPORT
  bool "Add ALSA plugins into root_fs"
  default n
  help
    Add ALSA plugins into file system

config CONFIG_AMBARELLA_ALSA_PLUGINS_PULSEAUDIO_SUPPORT
  depends on CONFIG_AMBARELLA_ALSA_PLUGINS_SUPPORT
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  bool "Add ALSA plugins for PulseAudio support into root_fs"
  default n
  help
    Add ALSA plugins for PulseAudio into file system

config CONFIG_AMBARELLA_ALSA_PLUGINS_LIBSAMPLERATE_SUPPORT
  depends on CONFIG_AMBARELLA_ALSA_PLUGINS_SUPPORT
  select CONFIG_AMBARELLA_LIBSAMPLERATE_SUPPORT
  bool "An external rate converter using libsamplerate"
  default n
  help
    An external rate converter using libsamplerate

config CONFIG_AMBARELLA_ALSA_PLUGINS_SPEEX_SUPPORT
  depends on CONFIG_AMBARELLA_ALSA_PLUGINS_SUPPORT
  select CONFIG_AMBARELLA_SPEEX_SUPPORT
  bool "An external rate converter using the Speex resampler"
  default n
  help
    An external rate converter using the Speex resmapler

endmenu

menu "PulseAudio"
config CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  select CONFIG_AMBARELLA_ALSA_SUPPORT
  select CONFIG_AMBARELLA_ALSA_UTILS_SUPPORT
  select CONFIG_AMBARELLA_ALSA_PLUGINS_SUPPORT
  select CONFIG_AMBARELLA_ALSA_PLUGINS_PULSEAUDIO_SUPPORT
  select CONFIG_AMBARELLA_JSONC_SUPPORT
  select CONFIG_AMBARELLA_LIBOGG_SUPPORT
  select CONFIG_AMBARELLA_LIBSNDFILE_SUPPORT
  select CONFIG_AMBARELLA_LIBTOOL_SUPPORT
  select CONFIG_AMBARELLA_SPEEX_SUPPORT
  select CONFIG_AMBARELLA_SPEEX_DSP_SUPPORT
  bool "Add PulseAudio into root_fs"
  default n
  help
    Add PulseAudio into file system

config CONFIG_AMBARELLA_PULSEAUDIO_SIMPLE_LIBRARY
  bool "Add PulseAudio Simple API library"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio Simple API library

config CONFIG_AMBARELLA_PULSEAUDIO_UTILS_SUPPORT
  bool "Add PulseAudio utilities into root_fs"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio utilities into root_fs

config CONFIG_AMBARELLA_PULSEAUDIO_AEC_SUPPORT
  bool "Add PulseAudio Echo Cancellation support"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  select CONFIG_AMBARELLA_WEBRTC_AUDIO_PROCESSING_SUPPORT
  default n
  help
    Add PulseAudio Echo Cancellation

config CONFIG_AMBARELLA_PULSEAUDIO_SIMPLE_PROTOCOL
  bool "Add PulseAudio Simple Protocol support"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio Simple Protocol support

config CONFIG_AMBARELLA_PULSEAUDIO_CLI_PROTOCOL
  bool "Add PulseAudio CLI Protocol support"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio Simple Protocol support

config CONFIG_AMBARELLA_PULSEAUDIO_HTTP_PROTOCOL
  bool "Add PulseAudio HTTP Protocol support"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio HTTP Protocol support

config CONFIG_AMBARELLA_PULSEAUDIO_RTP_PROTOCOL
  bool "Add PulseAudio RTP Protocol support"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio RTP Protocol support

config CONFIG_AMBARELLA_PULSEAUDIO_TUNNEL_SUPPORT
  bool "Add PulseAudio tunnel support"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio tunnel support

config CONFIG_AMBARELLA_PULSEAUDIO_COMBINE_SUPPORT
  bool "Add PulseAudio sink combine support"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio tunnel support

config CONFIG_AMBARELLA_PULSEAUDIO_REMAP_SUPPORT
  bool "Add PulseAudio sink remap support"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio tunnel support

config CONFIG_AMBARELLA_PULSEAUDIO_VOLUME_CONTROL_SUPPORT
  bool "Add PulseAudio volume control modules"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio volume control modules

config CONFIG_AMBARELLA_PULSEAUDIO_MISC_MODULES
  bool "Add PulseAudio misc modules"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio misc modules

config CONFIG_AMBARELLA_PULSEAUDIO_TEST_MODULES
  bool "Add PulseAudio test modules"
  depends on CONFIG_AMBARELLA_PULSEAUDIO_SUPPORT
  default n
  help
    Add PulseAudio pipe/null sink/source

endmenu

config CONFIG_AMBARELLA_SYSTEM_SOUND_FILES
  bool "Add sound files to root_fs"
  default n
  help
    Add the guide voices and effects to root_fs

endmenu

