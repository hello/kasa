config CONFIG_AMBARELLA_PINFO_SUPPORT
  bool "Add pinfo into root_fs"
  select CONFIG_AMBARELLA_NCURSES_SUPPORT
  default n
  help
    pinfo shows info document

config CONFIG_UTIL_LINUX_SUPPORT
  bool "Add util_linux into root_fs"
  default y
  help
    linux utilities

endmenu

